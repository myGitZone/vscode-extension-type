{
  "name": "ts-tpey",
	"displayName": "ts-tpey",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:ts-tpey.helloWorld",
		"onView:api-list"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ts-tpey.helloWorld",
				"title": "Hello World111"
			},
			{
				"command": "ts-tpey.refresh",
				"title": "Hello World111",
				"icon": "static/images/iconfont-gongyichongwu.svg"
			}
		],
		"menus": {
			"editor/title": [
                {
                    "when": "editorFocus && resourceLangId == javascript",
                    "command": "ts-tpey.helloWorld",
                    "group": "navigation",
					"icon": "static/images/typescript-def.svg"
                }
            ],
			"explorer/context": [
                {
					"command": "ts-tpey.helloWorld",
                    "group": "navigation"
                }
            ],
			"view/title": [
				{
					"command": "ts-tpey.refresh",
					"group": "navigation",
					"alt": "dddd",
					"icon": "static/images/iconfont-gongyichongwu.svg"
				  }
			]
		},
		"viewsContainers": {
            "activitybar": [
                {
                    "id": "generator-type",
                    "title": "类型生成",
                    "icon": "static/images/typescript-def.svg"
                }
            ]
        },
		"views": {
            "generator-type": [
                {
                    "id": "api-list",
                    "name": "列表"
                }
            ]
        }
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.66.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"eslint": "^8.11.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"typescript": "^4.5.5",
		"@vscode/test-electron": "^2.1.3"
	}
}
